{
  "name": "medtrack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "pnpm test:run && pnpm test:e2e",
    "test:api": "node scripts/test-api.js",
    "test:integration": "vitest run --config vitest.integration.config.js",
    "test:smoke": "playwright test --grep='@smoke'",
    "test:regression": "playwright test --grep='@regression'",
    "test:mobile": "playwright test --project=mobile",
    "test:accessibility": "playwright test --grep='@accessibility'",
    "test:performance": "playwright test --grep='@performance'",
    "test:security": "playwright test --grep='@security'",
    "test:visual": "playwright test --grep='@visual'",
    "test:cross-browser": "playwright test --project=chrome --project=firefox --project=safari",
    "test:parallel": "playwright test --workers=4",
    "test:ci": "vitest run --coverage --reporter=json && playwright test --reporter=json",
    "test:clean": "rm -rf coverage/ playwright-report/ test-results/",
    "test:audit": "pnpm audit --audit-level high",
    "test:types": "tsc --noEmit",
    "test:bundle": "npm run build && bundlesize",
    "test:lighthouse": "lighthouse-ci autorun",
    "pre-commit": "pnpm lint && pnpm test:run",
    "prepare-release": "pnpm test:clean && pnpm test:all && pnpm build",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:status": "supabase status",
    "db:studio": "supabase studio"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@supabase/supabase-js": "^2.49.10",
    "antd": "^5.25.4",
    "antd-phone-input": "^0.3.14",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.53.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.2.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
